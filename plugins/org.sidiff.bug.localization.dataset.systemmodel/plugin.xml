<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="testdriver"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.sidiff.bug.localization.dataset.systemmodel.TestDriverApplication">
         </run>
      </application>
   </extension>



   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated systemmodel -->
      <package
            uri="http://www.sidiff.org/systemmodel/1.0"
            class="org.sidiff.bug.localization.dataset.systemmodel.SystemModelPackage"
            genModel="model/systemmodel.genmodel"/>
   </extension>

</plugin>
