<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="testdriver"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.sidiff.bug.localization.dataset.modelview.TestDriverApplication">
         </run>
      </application>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated multiview -->
      <package
            uri="http://www.sidiff.org/dataset/multiview/1.0"
            class="org.sidiff.bug.localization.dataset.modelview.multiview.MultiviewPackage"
            genModel="model/multiview.genmodel"/>
   </extension>

</plugin>
