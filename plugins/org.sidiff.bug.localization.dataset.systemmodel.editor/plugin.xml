<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated systemmodel -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.sidiff.bug.localization.dataset.systemmodel.presentation.SystemModelModelWizardID"
            name="%_UI_SystemModelModelWizard_label"
            class="org.sidiff.bug.localization.dataset.systemmodel.presentation.SystemModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SystemModelModelFile.gif">
         <description>%_UI_SystemModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated systemmodel -->
      <editor
            id="org.sidiff.bug.localization.dataset.systemmodel.presentation.SystemModelEditorID"
            name="%_UI_SystemModelEditor_label"
            icon="icons/full/obj16/SystemModelModelFile.gif"
            extensions="systemmodel"
            class="org.sidiff.bug.localization.dataset.systemmodel.presentation.SystemModelEditor"
            contributorClass="org.sidiff.bug.localization.dataset.systemmodel.presentation.SystemModelActionBarContributor">
      </editor>
   </extension>

</plugin>
