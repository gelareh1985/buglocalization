<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated graph -->
      <package
            uri="http://www.sidiff.org/buglocalizationgraph/1.0"
            class="org.sidiff.bug.localization.dataset.graph.BugLocalizationGraphPackage"
            genModel="model/graph.genmodel"/>
   </extension>
   
   <extension
         id="buglocalizationgraph"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.sidiff.bug.localization.dataset.BugLocalizationGraphApplication">
         </run>
      </application>
   </extension>

</plugin>
